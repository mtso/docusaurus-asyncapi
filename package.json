    "clsx": "^1.2.1",
    "prism-react-renderer": "^1.3.5"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@docusaurus/module-type-aliases": "3.6.0",
    "@tsconfig/docusaurus": "^1.0.5",
    "typescript": "^4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
